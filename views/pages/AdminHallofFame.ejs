<body>
<%- include("./partials/AdminNav.ejs") %>
            <div class="app-content">
                <div class="content-wrapper event">
                    <div class="container">
                        <!-- ADD CODE HERE -->
                        <div class="row">
                            <div class="col">
                                <div class="page-description">
                                    <h1>Create Fame</h1>
                                </div>
                            </div>
                        </div>
                        <div>
                            <div class="row">
                                <div class="card-body">
                                    <form class="forms-sample" action="/admin/fames/post" method="POST" enctype="multipart/form-data">
                                      <div class="form-group">
                                        <label for="firstname">First Name</label>
                                        <input type="text" class="form-control" id="firstname" name="firstname" placeholder="Name">
                                      </div>
                                      <div class="form-group">
                                        <label for="lastname">Last Name</label>
                                        <input type="text" class="form-control" id="lastname" name="lastname" placeholder="Name">
                                      </div>
                                      <div>
                                        <label for="gender">Gender</label> <br>
                                        <div>
                                            <br>
                                            <select name="gender" id="gender">
                                                <option value="male"> Male</option>
                                                <option value="female">Female</option>
                                              </select>
                                        </div>
                                      </div>
                                      <br>
                                      <div class="form-group">
                                        <label for="famedescription">Description</label>
                                        <textarea class="form-control" id="famedescription" name="famedescription" rows="4"></textarea>
                                      </div>
                                      <div>
                                        <label for="division">Divison</label> <br>
                                        <div>
                                            <br>
                                            <select name="division" id="division">
                                                <option value="CBD">CBD </option>
                                                <option value="CPD">CPD</option>
                                                <option value="DEV">DEV</option>
                                              </select>
                                        </div>
                                        

                                      <label for="year">Year</label> <br>
                                        <div>
                                            <br>
                                            <select name="year" id="year">
                                                <option value="2017">2017 </option>
                                                <option value="2018">2018</option>
                                                <option value="2019">2019</option>
                                                <option value="2020">2020 </option>
                                                <option value="2021">2021</option>
                                                <option value="2022">2022</option>
                                              </select>
                                        </div>
                                   <div class="form-group">
                                        <label>Attach Photo</label>
                                        <div class="input-group col-xs-12">
                                            <input type="file" value="" id="famephoto" name="famephoto">
                                   </div>
                                      </div>
                                        <br>
                                      </div>

                                      

                                      
                                     
            
                                      <button>Post</button>
                                      <button class="btn btn-light">Cancel</button>
                                    </form>
                                  </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Javascripts -->
    <script src="../../assets/plugins/jquery/jquery-3.5.1.min.js"></script>
