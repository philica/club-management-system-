<body>
<%- include("./partials/AdminNav.ejs") %>
            <div class="app-content">
                <div class="content-wrapper event">
                    <div class="container">
                        <!-- ADD CODE HERE -->
                  
                        <div class="row">
                            <div class="col">
                                <div class="page-description">
                                    <h1>Events and Happenings</h1>
                                </div>
                                      
                            </div>
                        </div>
                        <div>
                            <div class="row">
                                <div class="card-body">
                            <form  action="/admin/event/upload" method="POST" enctype="multipart/form-data">
                                     <div class="form-group">
                                        <label for="title">Title</label>
                                        <input type="text" class="form-control" id="title" placeholder="Name" name="title">
                                      </div>
                                      <div>
                                        <label for="category">category</label> <br>
                                        <div>
                                            <br>
                                            <select name="category" id="category">
                                                <option value="event">Scheduled Events</option>
                                                <option value="news">NewsFeed</option>
                                              </select>
                                        </div>
                                      </div>
                                      <br>

                                      <div class="form-group">
                                        <label for="event_content">Description</label>
                                        <textarea class="form-control" id="event_content" name="event_content" rows="4"></textarea>
                                      </div>

                                      <div class="form-group">
                                        <label>Attach Photo</label>
                                        <div class="input-group col-xs-12">
                                            <input type="file" value="" id="picture" name="picture">
                                        </div>
                                      </div>
                                      <div>
                                           <label > Is This Event Post is Scoreboard</label>
                                            <br>
                                            <select name="scoreboard" id="tag">
                                                <option value="0" >No </option>
                                                <option value="1" >Yes</option>
                                              </select>
                                        </div>
                                      <div>
                                        <label for="tag">Tag</label> <br>
                                        <div>
                                            <br>
                                            <select name="tag" id="tag">
                                                <option value="CBD">CBD </option>
                                                <option value="CPD">CPD</option>
                                                <option value="DEV">DEV</option>
                                              </select>
                                        </div>
                                        <br>
                                      </div>
                                      <button>Submit</button>
     
                           </form>
                                  </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Javascripts -->
    <script src="../../assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="../../assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
    <script src="../../assets/plugins/pace/pace.min.js"></script>
    <script src="../../assets/js/custom.js"></script>
</body>
